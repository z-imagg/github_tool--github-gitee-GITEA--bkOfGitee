    "version": "0.2.0",
    "configurations": [

        ///////////////导入用到的运行配置//////////////

        {
            "name": "RepoRecurseImport.py【固定参数】",
            "type": "debugpy",
            "request": "launch",
            "program": "git_submodule_import_cmd_gen/RepoRecurseImport.py",
            "console": "integratedTerminal",
            "args": [  "--from_repo_url", "https://github.com/pytorch/pytorch.git","--from_commit_id","034921bd574cc84906b7996c07873454b7dd4135", "--goal_org", "ruut", "--sleep_seconds", "2" ],
            // https://github.com/onnx/onnx.git , https://github.com/pytorch/pytorch.git 
            // python /fridaAnlzAp/github-gitee-gitea/git_submodule_import_cmd_gen/gitSubmoduleImportCmdGen.py --parent_repo_dir /fridaAnlzAp/gitee/imagg/pytorch--pytorch  --goal_org imagg  --sleep_seconds 8 --sleep_seconds_delta 9
            "env": {
                "PYTHONPATH": "${workspaceFolder}:/fridaAnlzAp/github-gitee-gitea/py_util/"
            },
        },
        {
            "name": "Python调试【当前py文件携带参数】",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },


        ///////////////迁移用到的运行配置//////////////

        {
            "name": "运行submoduleMigrateToGitea.py",
            "type": "debugpy",
            "request": "launch",
            "program": "submoduleMigrateToGitea.py",
            "console": "integratedTerminal",
            "args": [
                "--from_parent_repo_dir",
                "/fridaAnlzAp/gitee/imagg/pytorch--pytorch/",
                "--mirror_base_ur",
                "https://gitee.com",
                "--mirror_org_name",
                "imagg"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}:/fridaAnlzAp/github-gitee-gitea/py_util/"
            },
        },
        {
            "name": "运行repoMigrateToGitea.py",
            "type": "debugpy",
            "request": "launch",
            "program": "repoMigrateToGitea.py",
            "console": "integratedTerminal",
            "args": [
                "--from_repo_url",
                "https://github.com/NVlabs/cub.git",
                "--mirror_base_ur",
                "https://gitee.com",
                "--mirror_org_name",
                "imagg"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}:/fridaAnlzAp/github-gitee-gitea/py_util/"
            },
